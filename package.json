{
  "name": "my-comp",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "scripts": {
    "build": "npm run clean; stencil build --docs && mv www/stencil-component docs",
    "clean": "rm -rf components dist docs hydrate loader www",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.15.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/store": "^1.5.0",
    "autoprefixer": "^10.4.0",
    "tailwindcss": "^3.0.7"
  },
  "license": "MIT",
  "dependencies": {
    "apexcharts": "^3.33.0",
    "sortablejs": "^1.15.0"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "types": "dist/types/components.d.ts"
}
